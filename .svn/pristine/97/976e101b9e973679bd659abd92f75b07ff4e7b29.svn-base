  top: 15px;
  left: 7px;
}
.floor em:before {
/*  position: absolute;
  content: '';
  top: 1px;
  left: -24px;
  content: "\e615";*/
  color: #cbcacf;
}
.floor i:last-child{
  border-bottom: 1px solid #e5e5e5;
}
