}
.floor:first-child:before {
  top: 25px;;
}
.floor:before {
  content: '';
  position: absolute;
  top: 0;
  bottom: 0;
  left: .9rem;
  width: 2px;
  background-color: #e0dfdf;
}
.floor em{
  position: relative;
}

.floor i:first-child{
  font-size: 16px;
